package object_d;

public class Main2 {

	public static void main(String[] args) {
		Hero h1 = new Hero("yamada");
		h1.hp = 100;
		Hero h2 = new Hero("sato");
		h2.hp = 100;
		Wizard w = new Wizard();
		w.name = "suga";
		w.hp = 50;
		w.heal(h1);
		w.heal(h2);
		w.heal(h2);

		System.out.println(h2.name + h2.hp);
		System.out.println(h1.name + h1.hp);
	}

}
