package object_d;

public class Main {

	public static void main(String[] args) {
		Sword s = new Sword();
		s.name = "flame Sword";
		s.damage = 10;

		Hero h = new Hero("minato");
		h.hp = 100;
		h.sword = s;

		System.out.println(h.sword.name);

		Matango m1 = new Matango();
		m1.hp = 50;
		m1.suffix = 'A';
		Matango m2 = new Matango();
		m2.hp = 40;
		m2.suffix = 'B';

	}

}
